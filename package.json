{
  "name": "seamstress-design",
  "private": true,
  "version": "1.0.0",
  "description": "AI-powered rapid prototyping system for OpenGov designers using Capital Design System",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenGov/seamstress-design.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "BASE_PATH=/seamstress-design vite build --mode production",
    "build:check": "tsc -b && BASE_PATH=/seamstress-design vite build --mode production",
    "build:analyze": "BASE_PATH=/seamstress-design vite build --mode production && vite-bundle-visualizer",
    "build:netlify": "BASE_PATH=/ vite build --mode production",
    "preview:netlify": "BASE_PATH=/ vite build --mode production && vite preview",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "./scripts/cleanup.sh",
    "clean:cache": "rm -rf .vite node_modules/.vite",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test-storybook": "test-storybook",
    "coverage": "vitest run --coverage",
    "deploy:storybook": "npm run build-storybook",
    "sync:figma": "node scripts/sync-figma-variables.js",
    "validate:tokens": "node scripts/validate-tokens.js",
    "figma:connect": "figma connect publish --token $FIGMA_ACCESS_TOKEN",
    "figma:connect:parse": "figma connect parse"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.4.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@grafana/faro-react": "^1.19.0",
    "@grafana/faro-web-sdk": "^1.19.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/lab": "^7.0.0-beta.16",
    "@mui/material": "^7.3.1",
    "@mui/system": "^7.3.2",
    "@mui/x-data-grid": "^8.11.2",
    "@mui/x-date-pickers": "^8.21.0",
    "@opengov/capital-mui-theme": "^37.9.4",
    "@opengov/capital-style": "^5.5.0-beta.3",
    "@opengov/components-ai-patterns": "^37.5.2",
    "@opengov/components-file-management": "^37.1.2",
    "@opengov/components-nav-bar": "^37.6.3",
    "@opengov/components-page-header": "^37.2.1",
    "@opengov/components-pagination": "^37.0.4",
    "@opengov/components-result": "^37.0.3",
    "@opengov/react-capital-assets": "^37.10.0",
    "@tiptap/core": "^3.4.4",
